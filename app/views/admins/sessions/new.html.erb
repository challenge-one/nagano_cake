<div class=session>
   <div class=session-contents>
<h1>管理者ログイン</h1>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :メールアドレス %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"text-field"%>
  </div>

  <div class="field">
    <%= f.label :パスワード,class:"field2" %>
    <%= f.password_field :password, autocomplete: "current-password",class:"text-field" %>
  </div>

  <div class="actions">
    <%= f.submit "ログイン",class:"btn-lg btn-primary" %>
  </div>
<% end %>
 </div>
</div>